<head>
	<meta charset="utf-8" />
	<meta name="description" content=""> 
	<meta name="author" content="">
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />

	<title>@ViewBag.Title</title>

	<!-- CSS Styles -->
	<link rel="stylesheet" href="@Url.Content("~/Content/css/style.css")" type="text/css">
	<link rel="stylesheet" href="@Url.Content("~/Content/css/colors.css")" type="text/css">
	<link rel="stylesheet" href="@Url.Content("~/Content/css/jquery.tipsy.css")" type="text/css">
	<link rel="stylesheet" href="@Url.Content("~/Content/css/jquery.wysiwyg.css")" type="text/css">
	<link rel="stylesheet" href="@Url.Content("~/Content/css/jquery.datatables.css")" type="text/css">
	<link rel="stylesheet" href="@Url.Content("~/Content/css/jquery.nyromodal.css")" type="text/css">
	<link rel="stylesheet" href="@Url.Content("~/Content/css/jquery.datepicker.css")" type="text/css">
	<link rel="stylesheet" href="@Url.Content("~/Content/css/jquery.fileinput.css")" type="text/css">
	<link rel="stylesheet" href="@Url.Content("~/Content/css/jquery.fullcalendar.css")" type="text/css">
	<link rel="stylesheet" href="@Url.Content("~/Content/css/jquery.visualize.css")" type="text/css">

	<!-- Google WebFonts --> 
	<link href="http://fonts.googleapis.com/css?family=PT+Sans:regular,italic,bold,bolditalic" rel="stylesheet" type="text/css"> 
</head>

<body>
	
	<!-- Fixed Layout Wrapper -->
	<div class="fixed-wraper">

	<!-- Aside Block -->
	<section role="navigation">

		<!-- Header with logo and headline -->
		<header>
			<a id="logolink" title="@_("Hayman Insights")"></a>
			<h1>@_("Headline")</h1>
		</header>
		<!-- /Header with logo and headline -->
		
		<!-- User Info -->
		@RenderPage("~/Views/Shared/Partial/_UserInfo.cshtml", new { DisplayName = "Anonymous", Role = "Administrator", FullName = "Anonymous" })
		<!--/ User Info -->
		
		<!-- Main Navigation -->
		@RenderPage("~/Views/Shared/Partial/_MainNavigation.cshtml")
		<!-- /Main Navigation -->
		
		<!-- Sidebar -->
		@RenderPage("~/Views/Shared/Partial/_Sidebar.cshtml")
		<!-- /Sidebar -->
	</section>
	<!-- /Aside Block -->
	
	<!-- Main Content -->
	<section role="main">
			
		@RenderBody()

	</section>
	<!-- /Main Content -->
	
	</div>
	<!-- /Fixed Layout Wrapper -->

</body>

<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.6.1/jquery.min.js"></script>
<script type="text/javascript">!window.jQuery && document.write(unescape('%3Cscript src="@Url.Content("~/Scripts/jquery/jquery-1.6.1.min.js")"%3E%3C/script%3E'))</script>

<script type="text/javascript" src="@Url.Content("~/Scripts/libs/selectivizr.js")"></script>
