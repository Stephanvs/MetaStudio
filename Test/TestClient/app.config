<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<system.runtime.serialization>
		<dataContractSerializer>
			<declaredTypes>
				<add type="Ncqrs.Commanding.CommandBase, Ncqrs, Version=0.8.0.0, Culture=neutral">
					<knownType type="Hayman.Commands.CreateModel, Hayman.Commands, Version=1.0.0.0, Culture=neutral"/>
					<knownType type="Hayman.Commands.AddMetaItem, Hayman.Commands, Version=1.0.0.0, Culture=neutral"/>
				</add>
			</declaredTypes>
		</dataContractSerializer>
	</system.runtime.serialization>
	<system.serviceModel>
		<bindings />
		<serviceHostingEnvironment aspNetCompatibilityEnabled="true" />
		<client>
			<endpoint address="http://localhost:1338/CommandService" binding="basicHttpBinding"
			  contract="Ncqrs.CommandService.Contracts.ICommandWebService"
			  name="CommandWebServiceClient" />
		</client>
		<behaviors>
			<serviceBehaviors>
				<behavior name="withMetadataAndDebugging">
					<serviceDebug includeExceptionDetailInFaults="true" />
					<serviceMetadata httpGetEnabled="true" />
				</behavior>
			</serviceBehaviors>
		</behaviors>
	</system.serviceModel>
</configuration>