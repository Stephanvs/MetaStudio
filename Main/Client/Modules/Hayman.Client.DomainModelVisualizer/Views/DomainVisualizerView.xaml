<infrastructurepresentationcontrols:WindowElement  x:Class="Hayman.Client.DomainModelVisualizerModule.Views.DomainVisualizerView"
			    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			    xmlns:controls="clr-namespace:Hayman.Client.DomainModelVisualizerModule.Controls"
                xmlns:infrastructurepresentationcontrols="clr-namespace:Hayman.Client.Presentation.Windows.Controls;assembly=Hayman.Client.Presentation.Windows">
    <Grid Background="White">
        
        <controls:PanAndZoomViewer>
            <controls:GraphLayout x:Name="layout" LayoutAlgorithmType="Tree" Graph="{Binding Path=Graph}" Background="White" />
        </controls:PanAndZoomViewer>
    </Grid>
</infrastructurepresentationcontrols:WindowElement>
