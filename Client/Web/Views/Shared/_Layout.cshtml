<script type="text/javascript" src="@Url.Content("~/Scripts/jquery/jquery.datatables.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/jquery/jquery.datepicker.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/jquery/jquery.fileinput.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/jquery/jquery.fullcalendar.min.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/jquery/excanvas.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/jquery/jquery.visualize.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/jquery/jquery.visualize.tooltip.js")"></script>

<script type="text/javascript" src="@Url.Content("~/Scripts/libs/json2.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/libs/history.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/libs/history.html4.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/libs/history.adapter.jquery.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/libs/amplify.min.js")"></script>
