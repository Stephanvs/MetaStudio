<?xml version="1.0"?>
<configuration>

  <configSections>
  </configSections>

  <connectionStrings>
    <add name="Hayman.Service.Properties.Settings.EventStoreConnectionString"
      connectionString="Data Source=MICHEL;Initial Catalog=HaymanEventStore;Integrated Security=True;MultipleActiveResultSets=True"
      providerName="" />
    <add name="HaymanReadModelEntities" connectionString="metadata=res://*/HaymanReadModel.csdl|res://*/HaymanReadModel.ssdl|res://*/HaymanReadModel.msl;provider=System.Data.SqlClient;provider connection string=&quot;Data Source=MICHEL;Initial Catalog=HaymanReadModel;Integrated Security=True;MultipleActiveResultSets=True&quot;" providerName="System.Data.EntityClient" />
  </connectionStrings>
  <system.serviceModel>
    <bindings/>
    <services>
      <service name="Ncqrs.CommandService.CommandWebService" behaviorConfiguration="withMetadataAndDebugging">
        <endpoint address="http://localhost:1338/CommandService"
                  binding="basicHttpBinding"
                  bindingNamespace="http://ncqrs.org/services/2010/12/"
                  contract="Ncqrs.CommandService.Contracts.ICommandWebService"/>
        <endpoint address="http://localhost:1338/CommandService/mex"
                  binding="mexHttpBinding"
                  contract="IMetadataExchange"/>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="withMetadataAndDebugging">
          <serviceDebug includeExceptionDetailInFaults="true"/>
          <serviceMetadata httpGetEnabled="false"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>


</configuration>
